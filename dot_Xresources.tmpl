Xcursor.size: 16

URxvt.font: xft:DejaVu Sans Mono for Powerline:size={{ .ui.default_terminal_font_size }}, xft:WenQuanYi Micro Hei Mono:regular:size={{ .ui.default_terminal_font_size }}:minspace=true

URxvt.scrollBar: false

URxvt.dpi: 96
URxvt.antialias: true
URxvt.rgba: rgb

URxvt.utf-8: true
URxvt.locale: true

URxvt.internalBorder: 10

! Base16 Bright
! Scheme: Chris Kempson (http://chriskempson.com)
!
URxvt.foreground:   #e0e0e0
#ifdef background_opacity
URxvt.background:   [background_opacity]#000000
URxvt.borderColor:  [background_opacity]#000000
#else
URxvt.background:   #000000
URxvt.borderColor:  #000000
#endif
URxvt.cursorColor:  #e0e0e0

URxvt.color0:       #000000
URxvt.color1:       #fb0120
URxvt.color2:       #a1c659
URxvt.color3:       #fda331
URxvt.color4:       #6fb3d2
URxvt.color5:       #d381c3
URxvt.color6:       #76c7b7
URxvt.color7:       #e0e0e0

URxvt.color8:       #b0b0b0
URxvt.color9:       #fb0120
URxvt.color10:      #a1c659
URxvt.color11:      #fda331
URxvt.color12:      #6fb3d2
URxvt.color13:      #d381c3
URxvt.color14:      #76c7b7
URxvt.color15:      #ffffff

! Note: colors beyond 15 might not be loaded (e.g., xterm, urxvt),
! use 'shell' template to set these if necessary
URxvt.color16:      #fc6d24
URxvt.color17:      #be643c
URxvt.color18:      #303030
URxvt.color19:      #505050
URxvt.color20:      #d0d0d0
URxvt.color21:      #f5f5f5

